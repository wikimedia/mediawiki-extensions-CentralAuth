[access]
	inheritFrom = mediawiki/extensions
[access "refs/*"]
	owner = group Administrators
	owner = group extension-CentralAuth
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[project]
	description = MediaWiki extension CentralAuth.
[allowSuperproject "mediawiki/extensions"]
	matching = refs/heads/master:refs/heads/master
